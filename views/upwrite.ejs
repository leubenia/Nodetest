    <title>르베니아 게시판 | 작성</title>
    <script>

        function up_write() {
            
            let title = $("#title").val()
            let name = $("#name").val()
            let body = $("#test").val()
            let pw = $("#pw").val()
            console.log(title,name,body,pw)
            $.ajax({
                type: "POST",
                url: `/api/write`,
                data: {title : title, name : name,  body : body , pw : pw},
                success: function (response) {
                    console.log(response["result"])
                }
            })
        }

    </script>
